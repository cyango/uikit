{
  "name": "html23",
  "type": "module",
  "description": "CLI for installing pmndrs/uikit user interfaces build with html23",
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/pmndrs/uikit",
  "author": "Bela Bohlender",
  "keywords": [
    "html23",
    "uikit",
    "three.js",
    "userinterface",
    "flexbox",
    "yoga",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:pmndrs/uikit.git"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "check:prettier": "prettier --check src scripts tests",
    "check:eslint": "eslint 'src/**/*.{tsx,ts}'",
    "fix:prettier": "prettier --write src scripts tests",
    "fix:eslint": "eslint 'src/**/*.{tsx,ts}' --fix"
  },
  "dependencies": {
    "@react-three/uikit": "workspace:^",
    "@react-three/uikit-default": "workspace:^",
    "@react-three/uikit-lucide": "workspace:^",
    "brotli-compress": "^1.3.3",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "firebase": "^10.11.1",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9"
  }
}
